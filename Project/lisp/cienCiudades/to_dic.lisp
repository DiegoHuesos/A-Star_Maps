(setq city '())
(setq lat '())
(setq lng '())

(defun to_dic (lst)
	(cond
		((null lst))
		(t 
			(push (caar lst) city) 
			(push (caadar lst) lat) 
			(push (cadr(cadar lst)) lng) 
			(to_dic (cdr lst))
		)
	)
)

(defun tostr (lst)
	(apply #'concatenate 'string 
		(mapcar #'(lambda (sim) (concatenate 'string (string sim) " ")) lst)
	)
)

(to_dic coordsInicial)

(with-open-file (str "./city.txt" 
                     :direction :output 
                     :if-exists :supersede 
                     :if-does-not-exist :create) 
    (format str (tostr city))) 

(defun write-numeric-list(filename l)
  (with-open-file (out filename :direction :output :if-exists :append :if-does-not-exist :create)
    (dolist (segment l)
      (format out "~D " segment))
    (format out "~%")))

(write-numeric-list "./lat.txt" lat)
(write-numeric-list "./lng.txt" lng)
